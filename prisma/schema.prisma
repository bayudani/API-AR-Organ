generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


model Organ {
  id          Int      @id @default(autoincrement())
  name        String   @unique // nama target di vuforia
  description String   @db.Text // @db.Text biar bisa muat teks panjang
  funFact     String?  @db.VarChar(500) 
  model3D_Url String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}